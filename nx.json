  },
  "projects": {
    "fos-scheduling-engine": { "tags": ["scope:scheduling", "type:app"] },
    "fos-scheduling-engine-e2e": {
      "tags": [],
      "implicitDependencies": ["fos-scheduling-engine"]
    }
  }
}
